<Window x:Class="CommandPattern.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Task Scheduler" Height="294" Width="457">
    <Grid Height="258" Width="424">
        <Button Height="23" Margin="0,0,111,18" Name="Load" VerticalAlignment="Bottom" Click="LoadIni" HorizontalAlignment="Right" Width="76">Load</Button>
        <TextBox Margin="32,37,0,63" Name="itemBox" HorizontalAlignment="Left" Width="179" VerticalScrollBarVisibility="Auto" Visibility="Visible" BorderBrush="Black" Background="White"></TextBox>
        <TextBox Margin="0,37,16,63" Name="msgbox" HorizontalScrollBarVisibility="Hidden" HorizontalAlignment="Right" Width="180" VerticalScrollBarVisibility="Auto" BorderBrush="Black" />
        <Label Height="28" HorizontalAlignment="Left" Margin="10,8,0,0" Name="Items" VerticalAlignment="Top" Width="120">Items</Label>
        <Label Height="28" Margin="192,8,111,0" Name="Messeage" VerticalAlignment="Top">Messeage</Label>
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,30,18" Name="exe" VerticalAlignment="Bottom" Width="75" Click="Executed">Execute</Button>
    </Grid>
</Window>
